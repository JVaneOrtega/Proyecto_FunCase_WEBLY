    <header class="header index2">
        <div class="navbar-area">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-lg-12">
                        <nav class="navbar navbar-expand-lg">
                            <a class="navbar-brand logo" href="index.html">
                                <img class="logo1" src="https://res.cloudinary.com/dj2f7ngpv/image/upload/v1629227087/vane/Logo_hc6hth.jpg" alt="Logo" style="width: 50px;">
                                <img class="logo2" src="https://res.cloudinary.com/dj2f7ngpv/image/upload/v1629227087/vane/Logo_hc6hth.jpg" alt="Logo" style="width: 50px;">

                            </a>
                            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                                <span class="toggler-icon"></span>
                                <span class="toggler-icon"></span>
                                <span class="toggler-icon"></span>
                            </button>
                            <div class="collapse navbar-collapse sub-menu-bar" id="navbarSupportedContent">
                                <ul id="nav" class="navbar-nav ml-auto">

                                    <li class="nav-item">
                                        @Html.ActionLink("Inicio", "Landing", "Home", null, htmlAttributes: new { @class = "nav-link" })
                                    </li>
                                    @if (Request.IsAuthenticated)
                                    {
                                        if (User.IsInRole("Diseñador"))
                                        {
                                            <li class="nav-item">@Html.ActionLink("Mis datos", "Index", "Designer", null, htmlAttributes: new { @class = "nav-link" })</li>
                                            <li class="nav-item">@Html.ActionLink("Mis imágenes", "Index", "Imagenes", null, htmlAttributes: new { @class = "nav-link" })</li>
                                        }
                                        else if (User.IsInRole("Cliente"))
                                        {
                                            <li class="nav-item">@Html.ActionLink("Mis datos", "Index", "Cliente", null, htmlAttributes: new { @class = "nav-link" })</li>
                                            <li class="nav-item">@Html.ActionLink("Mis pedidos", "Index", "Pedidos", null, htmlAttributes: new { @class = "nav-link" })</li>
                                            <li class="nav-item">@Html.ActionLink("Mis direcciones", "Index", "Direccion", null, htmlAttributes: new { @class = "nav-link" })</li>
                                        }
                                        else if (User.IsInRole("Admin"))
                                        {
                                            <li class="nav-item">@Html.ActionLink("Diseñadores", "Index", "Designer", null, htmlAttributes: new { @class = "nav-link" })</li>
                                            <li class="nav-item">@Html.ActionLink("Clientes", "Index", "Cliente", null, htmlAttributes: new { @class = "nav-link" })</li>
                                            <li class="nav-item">@Html.ActionLink("Usuarios", "Index", "UsersAdmin", null, htmlAttributes: new { @class = "nav-link" })</li>
                                            <li class="nav-item">@Html.ActionLink("Roles", "Index", "RolesAdmin", null, htmlAttributes: new { @class = "nav-link" })</li>
                                        }
                                        else if (User.IsInRole("Empleado"))
                                        {
                                            <li class="nav-item">@Html.ActionLink("Marcas", "Index", "Marcas", null, htmlAttributes: new { @class = "nav-link" })</li>
                                            <li class="nav-item">@Html.ActionLink("Modelos", "Index", "Modeloes", null, htmlAttributes: new { @class = "nav-link" })</li>
                                            <li class="nav-item">@Html.ActionLink("Materiales", "Index", "Materiales", null, htmlAttributes: new { @class = "nav-link" })</li>
                                            <li class="nav-item">@Html.ActionLink("Pedidos", "Index", "Pedidos", null, htmlAttributes: new { @class = "nav-link" })</li>
                                            <li class="nav-item">@Html.ActionLink("Clientes", "Index", "Cliente", null, htmlAttributes: new { @class = "nav-link" })</li>
                                        }
                                    }
                                    <li class="nav-item"><a href="@Url.Action("AgregaCarrito", "Carrito")" class="nav-link"><img class="img-responsive" style="height: 20px;" src="https://res.cloudinary.com/dj2f7ngpv/image/upload/v1629227088/vane/carrito_csmivj.png" /></a></li>
                                    @Html.Partial("_LoginPartial")
                                </ul>
                            </div>
